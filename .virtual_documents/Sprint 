N=15

# rest of this I want at the end to print out the factorial of N

total=0
for value in range(N):
    total=total*value

print(total) 








N=15

# rest of this I want at the end to print out the factorial of N

total=0
for value in range(1,N+1):
    print(value)
    total=total*value

print(total) 


N=15

# rest of this I want at the end to print out the factorial of N

total=0
for value in range(N):
    print(value)
    total=total*(value+1)

print(total) 





N=15

# rest of this I want at the end to print out the factorial of N

total=1
for value in range(N):
    total=total*(value+1)

print(total) 


def factorial(N):
    total=1
    for value in range(N):
        total=total*(value+1)

    return total


a=factorial(5)


print(a)





